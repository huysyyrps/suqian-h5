.apply-flex{
	display: flex;
}
.apply-flex-row{
	flex-direction: row;
}
.apply-flex-column{
	flex-direction: column;
}

.apply-align-center{
	align-items: center;
}

.apply-justify-between{
	justify-content: space-between;
}
.apply-justify-center{
	justify-content: center;
}

.apply-base-fontSize{
	font-size: 30rpx;
}

.apply-text-right{
	text-align: right;
}

.apply-margin-top{
	margin-top: 20rpx;
}
.button-bottom{
		padding-bottom: 0;  
		padding-bottom: constant(safe-area-inset-bottom);  
		padding-bottom: env(safe-area-inset-bottom);  
	}
